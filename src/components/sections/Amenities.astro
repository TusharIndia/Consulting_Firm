---
const {img, heading, description} = Astro.props.Content;
---

<section class="bg-base-50 section">
    <div class="container program-content-width">
        <h2 class="mb-5">
            {heading}
        </h2>
        <p class="mb-5">
            {description}
        </p>
        <img
            src={img}
            alt=""
            class="w-full max-h-[430px] rounded-xl object-cover"
        />
    </div>
</section>