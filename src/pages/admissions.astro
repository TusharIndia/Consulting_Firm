---
import AdmissionNextStep from '../components/AdmissionNextStep.astro';
import FAQ from '../components/sections/FAQ.astro';
import AdmissionSteps from '../components/sections/AdmissionSteps.astro';
import PDF from '../components/sections/PDF.astro';
import Map from '../components/Map.astro';
import { fetchGeneralData, fetchAdmissions } from '../utils/dataMapping.ts';
import MainLayout from '../layouts/MainLayout.astro';
import CTA from '../components/CTA.astro';
import HeroVideo from '../components/sections/HeroVideo.astro';
const GeneralData = await fetchGeneralData();
const { data: AdmissionsData } = await fetchAdmissions();
---

<MainLayout title="Admissions" description="Alcohol and Drug Rehab in Boca Raton, Florida">
    <HeroVideo Content={AdmissionsData.hero} GeneralData={GeneralData} />
    <AdmissionSteps />
    <PDF Content={AdmissionsData.pdf} />
    <AdmissionNextStep Content={AdmissionsData.nextSteps} />
    <FAQ Content={AdmissionsData.faq}/>
    <Map />
    <CTA Content={AdmissionsData.cta}/>
</MainLayout>
