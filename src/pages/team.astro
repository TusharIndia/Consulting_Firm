---
import { fetchGeneralData, fetchTeamData } from '../utils/dataMapping.ts';
import MainLayout from "../layouts/MainLayout.astro"
import Hero from '../components/layout/Hero.astro';
import TeamSection from '../components/Team.astro';
import CTA from '../components/CTA.astro';

const GeneralData = await fetchGeneralData();
const { data: TeamData } = await fetchTeamData();
---

<MainLayout title="Team" description="Our team">
	<Hero Content={TeamData.hero} GeneralData={GeneralData}/>
    <TeamSection Content={TeamData.team} />
    <CTA Content={TeamData.cta} />
</MainLayout>