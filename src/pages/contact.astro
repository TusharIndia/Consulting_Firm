---
import { fetchGeneralData, fetchContactData } from '../utils/dataMapping.ts';

import Testimonials from "../components/sections/Testimonials.astro";

import Map from "../components/Map.astro";
import Hero from "../components/layout/Hero.astro";
import MainLayout from "../layouts/MainLayout.astro";
import ContactUs from "../components/sections/ContactUs.astro";
import ContactCards from "../components/sections/ContactCards.astro";
const GeneralData = await fetchGeneralData();
const {data:ContactData} = await fetchContactData();

import ContactHero from '../components/sections/ContactHero.astro';
import HeroProgram from '../components/sections/HeroProgram.astro';
---

<MainLayout title="Contact Us">

    <!-- Form Section -->
     <HeroProgram Content={ContactData.hero} GeneralData={GeneralData} />

    <ContactHero contactData={ContactData.contactHero} />
    <!-- Contact Box  -->

	<Testimonials Content={ContactData.testimonials} />

    <!-- Map -->

</MainLayout>
